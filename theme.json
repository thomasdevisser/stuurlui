{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"typography": {
			"fluid": true,
			"fontFamilies": [
				{
					"name": "Ubuntu",
					"slug": "ubuntu",
					"fontFamily": "Ubuntu, sans-serif",
					"fontFace": [
						{
							"fontFamily": "Ubuntu",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [
								"file:./assets/fonts/ubuntu-regular.woff2"
							]
						},
						{
							"fontFamily": "Ubuntu",
							"fontWeight": "700",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [
								"file:./assets/fonts/ubuntu-700.woff2"
							]
						}
					]
				}
			],
			"fontSizes": [
				{
					"name": "Heading 1",
					"size": "2.875rem",
					"fluid": {
						"min": "2rem",
						"max": "2.875rem"
					},
					"slug": "heading-1"
				},
				{
					"name": "Heading 2",
					"size": "2rem",
					"fluid": {
						"min": "1.625rem",
						"max": "2rem"
					},
					"slug": "heading-2"
				},
				{
					"name": "Heading 3",
					"size": "1.625rem",
					"fluid": {
						"min": "1.375rem",
						"max": "1.625rem"
					},
					"slug": "heading-3"
				},
				{
					"name": "Heading 4",
					"size": "1.375rem",
					"fluid": {
						"min": "1.125rem",
						"max": "1.375rem"
					},
					"slug": "heading-4"
				},
				{
					"name": "Heading 5",
					"size": "1.125rem",
					"fluid": {
						"min": "1rem",
						"max": "1.125rem"
					},
					"slug": "heading-5"
				},
				{
					"name": "Body",
					"size": "1.125rem",
					"fluid": {
						"min": "1rem",
						"max": "1.125rem"
					},
					"slug": "body"
				},
				{
					"name": "Body Small",
					"size": "0.875rem",
					"slug": "body-s"
				}
			]
		},
		"color": {
			"defaultPalette": false,
			"palette": [
				{
					"name": "Primary",
					"slug": "primary",
					"color": "#243363"
				},
				{
					"name": "Secondary",
					"slug": "secondary",
					"color": "#3dde96"
				},
				{
					"name": "Tertiary",
					"slug": "tertiary",
					"color": "#47b3c2"
				},
				{
					"name": "Quaternary",
					"slug": "quaternary",
					"color": "#5547c2"
				},
				{
					"name": "White",
					"slug": "white",
					"color": "#fff"
				}
			]
		},
		"layout": {
			"contentSize": "1168px"
		},
		"spacing": {
			"customSpacingSize": false
		}
	},
	"styles": {
		"spacing": {
			"padding": {
				"left": "20px",
				"right": "20px"
			}
		},
		"typography": {
			"lineHeight": "1.6",
			"fontFamily": "var(--wp--preset--font-family--ubuntu)",
			"fontSize": "var(--wp--preset--font-size--body)"
		},
		"color": {
			"background": "var(--wp--preset--color--primary)",
			"text": "var(--wp--preset--color--white)"
		},
		"elements": {
			"heading": {
				"typography": {
					"lineHeight": "1.4"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--tertiary)"
				},
				":hover": {
					"typography": {
						"textDecoration": "none"
					}
				},
				":visited": {
					"color": {
						"text": "var(--wp--preset--color--tertiary)"
					}
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-1)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-2)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-3)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-4)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-5)"
				}
			}
		},
		"blocks": {
			"core/button": {
				"typography": {
					"textTransform": "uppercase",
					"fontWeight": "700"
				},
				"color": {
					"background": "var(--wp--preset--color--secondary)",
					"text": "var(--wp--preset--color--primary)"
				},
				"shadow": "0px 1px 20px #3DDE9699"
			},
			"core/navigation-link": {
				"border": {
					"bottom": {
						"color": "#E8EAEF",
						"width": "1px"
					}
				},
				"spacing": {
					"margin": {
						"left": "18px",
						"right": "18px"
					}
				}
			}
		}
	},
	"customTemplates": [],
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		}
	]
}

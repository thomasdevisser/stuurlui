{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"typography": {
			"fluid": true,
			"fontFamilies": [
				{
					"name": "Ubuntu",
					"slug": "ubuntu",
					"fontFamily": "Ubuntu, sans-serif",
					"fontFace": [
						{
							"fontFamily": "Ubuntu",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [
								"file:./assets/fonts/ubuntu-regular.woff2"
							]
						},
						{
							"fontFamily": "Ubuntu",
							"fontWeight": "700",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [
								"file:./assets/fonts/ubuntu-700.woff2"
							]
						}
					]
				}
			],
			"fontSizes": [
				{
					"name": "Heading 1",
					"size": "2.875rem",
					"fluid": {
						"min": "2rem",
						"max": "2.875rem"
					},
					"slug": "heading-1"
				},
				{
					"name": "Heading 2",
					"size": "2rem",
					"fluid": {
						"min": "1.625rem",
						"max": "2rem"
					},
					"slug": "heading-2"
				},
				{
					"name": "Heading 3",
					"size": "1.625rem",
					"fluid": {
						"min": "1.375rem",
						"max": "1.625rem"
					},
					"slug": "heading-3"
				},
				{
					"name": "Heading 4",
					"size": "1.375rem",
					"fluid": {
						"min": "1.125rem",
						"max": "1.375rem"
					},
					"slug": "heading-4"
				},
				{
					"name": "Heading 5",
					"size": "1.125rem",
					"fluid": {
						"min": "1rem",
						"max": "1.125rem"
					},
					"slug": "heading-5"
				},
				{
					"name": "Body",
					"size": "1.125rem",
					"fluid": {
						"min": "1rem",
						"max": "1.125rem"
					},
					"slug": "body"
				},
				{
					"name": "Body Small",
					"size": "0.875rem",
					"slug": "body-s"
				}
			]
		},
		"color": {
			"defaultPalette": false,
			"palette": []
		},
		"layout": {},
		"spacing": {
			"customSpacingSize": false
		}
	},
	"styles": {
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--ubuntu)",
			"fontSize": "var(--wp--preset--font-size--body)"
		},
		"color": {},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-1)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-2)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-3)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-4)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-5)"
				}
			}
		},
		"blocks": {
			"core/button": {
				"typography": {
					"textTransform": "uppercase"
				}
			}
		}
	},
	"customTemplates": [],
	"templateParts": []
}

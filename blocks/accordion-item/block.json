{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "strl/accordion-item",
	"version": "0.1.0",
	"title": "Accordion Item",
	"category": "theme",
	"icon": "smiley",
	"parent": ["strl/accordion"],
	"description": "A block showing a selected accordion item, to practice retrieving and selecting posts",
	"supports": {
		"html": false
	},
	"attributes": {
		"title": {
			"type": "string"
		},
		"content": {
			"type": "string"
		},
		"isOpen": {
			"type": "boolean",
			"default": false
		}
	},
	"textdomain": "strl",
	"editorScript": "file:./../../build/accordionItem.js",
	"editorStyle": "file:./../../build/accordionItem.css",
	"style": "file:./../../build/style-accordionItem.css",
	"render": "file:./render.php"
}
